{
  "routes": [
    {
      "src": "/img/(.*)",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/_vercel/image/(.*)",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    }
  ],
  "build": {
    "env": {
      "NUXT_IMAGE_PROVIDER": "vercel"
    }
  }
}
